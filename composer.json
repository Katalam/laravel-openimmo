{
    "name": "katalam/laravel-openimmo",
    "description": "This is my package laravel-openimmo",
    "keywords": [
        "Katalam",
        "laravel",
        "laravel-openimmo"
    ],
    "homepage": "https://github.com/katalam/laravel-openimmo",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Görß",
            "email": "bruno.goerss@inno-brain.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "doctrine/annotations": "^2.0",
        "goetas-webservices/xsd-reader": "*",
        "goetas-webservices/xsd2php": "*",
        "illuminate/contracts": "^10.0||^11.0",
        "jms/serializer": "^2.1 || ^3.5",
        "spatie/laravel-package-tools": "^1.16",
        "symfony/serializer": "^5.3 || ^6.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.0.2",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^v3.0.4",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^v3.6.0",
        "pestphp/pest-plugin-arch": "^v3.0.0",
        "pestphp/pest-plugin-laravel": "^v3.0.0",
        "phpstan/extension-installer": "^1.4.2",
        "phpstan/phpstan-deprecation-rules": "^2.0.1",
        "phpstan/phpstan-phpunit": "^2.0.4",
        "ext-json": "*",
        "phpstan/phpstan": "^1.0 || ^2.0",
        "infection/infection": ">=0.16",
        "friendsofphp/php-cs-fixer": "^3.0",
        "rector/rector": "2.*",
        "nette/php-generator": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Katalam\\OpenImmo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katalam\\OpenImmo\\Tests\\": "tests/",
            "Workbench\\App\\": "skeleton/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-openimmo --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest --parallel",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "prepare-release": "vendor/bin/pint && vendor/bin/rector"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Katalam\\OpenImmo\\OpenImmoServiceProvider"
            ],
            "aliases": {
                "LaravelOpenimmo": "Katalam\\OpenImmo\\Facades\\LaravelOpenimmo"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
